<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cgb.dao.UserDao">

    <select id="findUser" resultType="UserPojo">
        SELECT
            id,
            name,
            age,
            sex
        FROM
            user
    </select>

</mapper>
    